<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default' ">
    <StartAction>Project</StartAction>
    <StartProgram></StartProgram>
    <StartArguments>-t</StartArguments>
    <ExternalConsole>true</ExternalConsole>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'lan-client' ">
    <StartAction>Project</StartAction>
    <StartArguments>-t -n=lan_client -db=lan_client</StartArguments>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'lan-host' ">
    <StartAction>Project</StartAction>
    <StartArguments>-t -n=lan_host -db=lan_host</StartArguments>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'no-network' ">
    <StartAction>Project</StartAction>
    <StartArguments>-d -t</StartArguments>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'testnet' ">
    <StartAction>Project</StartAction>
    <StartArguments>-t -n=testnet_client</StartArguments>
    <ConsolePause>false</ConsolePause>
  </PropertyGroup>
</Project>