<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="dbDir" value="db" />
    <add key="walletDb" value="default" />
    <add key="blockchainDb" value="blockchain" />
    <add key="network" value="demo_client" />
    <add key="assetsDir" value="assets" />
    <add key="assetsDiscovery" value="demo.zenprotocol.com" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="fstar" value="../../../tools/fstar/mono"/>
    <add key="zulib" value="../../../Zulib/fstar"/>
  </appSettings>
  <system.diagnostics>
    <sources>
      <source name="Infrastructure">
        <listeners>
          <add name="TraceList" />
          <add name="TraceFile" />
        </listeners>
      </source>
      <source name="Wallet">
        <listeners>
          <add name="TraceList" />
          <add name="TraceFile" />
        </listeners>
      </source>
      <source name="Network">
        <listeners>
          <add name="TraceList" />
          <add name="TraceFile" />
        </listeners>
      </source>
      <source name="BlockChain">
        <listeners>
          <add name="TraceList" />
          <add name="TraceFile" />
        </listeners>
      </source>
      <source name="Miner">
        <listeners>
          <add name="TraceList" />
          <add name="TraceFile" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="Infrastructure" value="All"/>
      <add name="Wallet" value="All"/>
      <add name="Network" value="All"/>
      <add name="BlockChain" value="All"/>
      <add name="Miner" value="All"/>
    </switches>
    <sharedListeners>
      <!--add name="ColoredConsole" type="Infrastructure.Testing.ColorConsoleTraceListener, Infrastructure.Testing, Version=0.6.0.0, Culture=neutral, PublicKeyToken=null" /-->
      <add name="TraceList" type="Zen.AppTraceListener, zen, Version=0.6.0.0, Culture=neutral, PublicKeyToken=null" />
      <add name="TraceFile" 
           type="System.Diagnostics.TextWriterTraceListener" 
           initializeData="trace.log" />
    </sharedListeners>
    <trace autoflush="true" indentsize="4"/>
  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="FSharp.Core" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.4.1.0" newVersion="4.4.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Mono.Cairo" publicKeyToken="0738eb9f132ed756" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />
      </dependentAssembly>

      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>

    </assemblyBinding>



  </runtime>
</configuration>
